    "fast-safe-stringify": "^2.1.1",
    "logform": "^2.5.1",
    "nest-winston": "^1.9.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "@redtea/format-axios-error": "^2.1.1"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.2",
    "eslint-config-server": "workspace:*",
    "tsconfig": "workspace:*"
  }
