{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "NodeJS",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ESNext"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ES2020",
    "outDir": "lib"
  },
  "ts-node": {
    "require": ["tsconfig-paths/register"]
  }
}
