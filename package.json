{
  "name": "mono-ts",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "shared/*",
    "projects/main/packages/*",
    "projects/main/ui/*",
    "projects/bedkingdom/apps/*"
  ],
  "scripts": {
    "start:prod": "pnpm --filter @stock/apps-api start:prod",
    "build": "pnpm run stock:build",
    "stock": "___________________________STOCK___________________________",
    "stock:build": "turbo run build --no-daemon --filter=@stock/apps-*",
    "stock:web:dev": "turbo run dev --no-daemon --filter=@stock/apps-frontend",
    "stock:api:dev": "turbo run dev --no-daemon --filter=@stock/apps-api",
    "stock:web:build": "turbo run build --no-daemon --filter=@stock/apps-frontend",
    "stock:dev": "turbo run dev --no-daemon --filter=@stock/apps-*",
    "stock:start": "turbo run start --no-daemon --filter=@stock/apps-*",
    "stock:start:debug": "turbo run start:debug --no-daemon --filter=@stock/apps-*",
    "main": "___________________________MAIN___________________________",
    "dev:api": "turbo run dev --no-daemon --filter=nestjs",
    "dev:proxy": "turbo run dev --no-daemon --filter=@main/apps-proxy-sls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "watch": "turbo run watch --parallel --no-daemon"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/core": "^10.4.20",
    "@nestjs/platform-express": "^10.4.20",
    "@prisma/client": "^6.13.0",
    "axios": "1.8.2",
    "date-fns": "^4.1.0",
    "immutable": "^4.3.7",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "node-fetch": "^2.7.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "^7.8.2",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.121",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "chalk": "5.3.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "@eslint/eslintrc": "^3",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "4.2.0",
    "turbo": "^2.5.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "pnpm": {
    "overrides": {
      "@nestjs/common": "^10.3.0",
      "@nestjs/core": "^10.3.0",
      "@nestjs/platform-express": "^10.3.0",
      "reflect-metadata": "0.2.2"
    }
  }
}
