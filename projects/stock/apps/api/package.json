{
  "name": "@stock/apps-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "pnpm run prisma:generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma": "___________________________ PRISMA ___________________________",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "dotenv -e .env.default prisma migrate dev",
    "prisma:deploy": "dotenv -e .env.production -- prisma migrate deploy"
  },
  "dependencies": {
    "@nest/base": "workspace:*",
    "@nest/rabbitmq": "workspace:*",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/schedule": "^3.0.1",
    "@prisma/client": "^5.1.1",
    "@web/base": "workspace:*",
    "axios": "^1.4.0",
    "chitility": "workspace:*",
    "dotenv-cli": "^7.2.1",
    "moment": "^2.29.4",
    "node-html-parser": "^6.1.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.10",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.3",
    "@types/supertest": "^2.0.11",
    "eslint-config-server": "workspace:*",
    "jest": "29.6.2",
    "prisma": "^5.1.1",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@stock/stock-info(|/.*)$": "<rootDir>/libs/stock-info/src/$1"
    }
  }
}
