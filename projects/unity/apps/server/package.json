{
  "private": true,
  "name": "@unity/apps-server",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "build/index.js",
  "engines": {
    "node": ">= 20.9.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "schema-codegen": "schema-codegen src/modules/declaration/scheme.ts --csharp --output ../server/codegen-csharp/ --namespace Etheron.Colyseus.Schemas",
    "schema-help": "schema-codegen --help",
    "loadtest": "tsx loadtest/example.ts --room my_room --numClients 2",
    "build": "npm run clean && tsc",
    "clean": "rimraf build",
    "test": "mocha -r tsx test/**_test.ts --exit --timeout 15000",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.16.0",
    "@colyseus/testing": "^0.16.0",
    "@types/express": "^4.17.1",
    "@types/mocha": "^10.0.1",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.10.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@colyseus/auth": "^0.16.6",
    "@colyseus/core": "^0.16.16",
    "@colyseus/monitor": "^0.16.0",
    "@colyseus/playground": "^0.16.0",
    "@colyseus/schema": "^3.0.35",
    "@colyseus/tools": "^0.16.0",
    "colyseus": "^0.16.0",
    "eslint-config-server": "workspace:*",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "uuid": "^9.0.0"
  }
}
