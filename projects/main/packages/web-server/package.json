  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fast-safe-stringify": "^2.1.1",
    "logform": "^2.5.1",
    "serve-static": "^1.15.0",
    "splunk-logging": "^0.11.1",
    "url": "^0.11.0",
    "util": "^0.12.5",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
